{
  "tasks": {
    "corpo": "deno run --env-file --allow-net --allow-env --allow-run=npx --allow-write=./workflows --allow-read=./workflows,./secrets.json src/index.ts",
    "debug": "deno run --inspect --env-file --allow-net --allow-env --allow-run=npx --allow-write=./workflows --allow-read=./workflows,./secrets.json src/index.ts",
    "test": "deno test --env-file --allow-net --allow-env --allow-run=npx --allow-write=./workflows --allow-read=./workflows,./secrets.json,./resources,./src",
    "run": "deno task corpo run",
    "record": "deno task corpo record",
    "run-auto": "deno task corpo run-auto"
  },
  "imports": {
    "@ai-sdk/google": "npm:@ai-sdk/google@^2.0.7",
    "@ai-sdk/openai": "npm:@ai-sdk/openai@^2.0.16",
    "@ai-sdk/provider-utils": "npm:@ai-sdk/provider-utils@^3.0.4",
    "@modelcontextprotocol/sdk": "npm:@modelcontextprotocol/sdk@^1.17.3",
    "@openrouter/ai-sdk-provider": "npm:@openrouter/ai-sdk-provider@^1.1.2",
    "@std/expect": "jsr:@std/expect@^1.0.17",
    "@std/fmt": "jsr:@std/fmt@^1.0.8",
    "@std/path": "jsr:@std/path@^1.1.2",
    "@std/testing": "jsr:@std/testing@^1.0.15",
    "@std/yaml": "jsr:@std/yaml@^1.0.9",
    "ai": "npm:ai@^5.0.17",
    "commander": "npm:commander@^14.0.0",
    "zod": "npm:zod@^4.0.17"
